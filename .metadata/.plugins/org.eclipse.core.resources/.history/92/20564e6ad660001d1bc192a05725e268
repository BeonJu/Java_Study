package Q.ch05;

public class Question01 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
//		8.	주어진 배열 항목에서 최대값을 출력하는 코드를 작성해보세요(for 문 이용).
        int[] array = { 1, 5, 3, 8, 2 };
        int temp = 0;
        
        for(int i = 0; i>array.length; i++) {
        	if(temp > array[i]) {
        		temp = array[i];
        	}
        }
        System.out.println("최대값: "+temp);

	}

}
