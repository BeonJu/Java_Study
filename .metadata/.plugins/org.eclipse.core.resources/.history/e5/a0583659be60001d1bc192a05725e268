package Ex.chapter1_01;

public class SringMethod {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

			String ssn  = "910404122222";
			char gender = ssn.charAt(6);
			
			switch(gender) {
			case '1';
			case '3';
				System.out.println("남자");
				break;
			}
	}

}
